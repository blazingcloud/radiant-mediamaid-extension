

<%= link_to 'add media', :action => "new" %>

<table>
<% @mediamaids.each do |mediamaid| %>
	<tr>
		<td><%= link_to image_tag (mediamaid.mediamaid.url(:thumb)), :action => "show", :id  => mediamaid%> </td>
		<td><%= link_to 'Destroy', mediamaid, :confirm => 'Are you sure?', :method  => :delete %> </td>
	</tr>
<% end %>
</table>
		